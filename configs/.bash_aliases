alias v='vim'

# git
alias g='git'
alias gst='g st --ignore-submodules=all'
alias gbr='g br -a'
alias gdiff='g diff --ignore-submodules=all'
alias gdc='g dc'
alias gci='g ci'
alias gco='g co'
alias glg='g lg'
alias glg2='g lg2'

# docker
alias docker-stop-all='docker stop $(docker ps -a -q)'
alias docker-rm-all='docker rm $(docker ps -a -q)'
